    "def squaresum(n) : \n",
    "  \n",
    "    sm = 0\n",
    "    for i in range(1, n+1) : \n",
    "        sm = sm + (i * i) \n",
    "      \n",
    "    return sm \n",
    "  \n",
    " \n",
    "n = int(input(\"enter the number\")) \n",
    "\n",
    "print(\"the sum of square of first natural numbers is \",squaresum(n)) \n",
    "  "
   ]
  }
